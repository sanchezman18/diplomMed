@model IEnumerable<diplomMed.Models.Stretcher>

@{
    ViewData["Title"] = "Index";
}



<!--s<p>
    <a asp-action="Create">Create New</a>
</p>-->
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.WorkingCondition)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FoldedCondition)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Equip)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.WorkingCondition)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FoldedCondition)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Equip.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
